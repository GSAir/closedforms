// # recursive-simple/afterrec_false-unreach-call_true-termination.c
// # custom traverser
  t_void __VERIFIER_error()
  t_void f(t_int n){
    if (op_lessThan n 3) return
    else {}
    (op_postFixDecr n)
    f(n)
    ERROR:
    __VERIFIER_error()
  }
  t_int main(t_void ){
    f(4)
  }
// # default pretty printer
  extern void __VERIFIER_error();
  void f(int n)
  {
    if(n < 3)
    return;
    n--;
    f(n);
    ERROR:
    __VERIFIER_error();
  }
  int main(void)
  {
    f(4);
  }
