  t_int fibo(t_int n){
    if (op_lessThan n 1) {
      return 0
    }
    else if (op_equals n 1) {
      return 1
    }
    else {
      return (op_plus fibo((op_minus n 1)) fibo((op_minus n 2)))
    }
  }
  t_int main(t_void ){
    t_int x = 5
    t_int result = fibo(x)
    if (op_notequals result 5) {
