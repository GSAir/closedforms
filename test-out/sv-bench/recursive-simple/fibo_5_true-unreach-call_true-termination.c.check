  }
// # default pretty printer
  extern int __VERIFIER_nondet_int();
  extern void __VERIFIER_error();
  int fibo(int n)
  {
    if(n < 1){
      return 0;
    }else
    if(n == 1){
      return 1;
    }else{
      return fibo(n - 1) + fibo(n - 2);
    }
  }
  int main(void)
  {
    int x = 5;
    int result = fibo(x);
    if(result != 5){
      ERROR:
      __VERIFIER_error();
    }
    return 0;
  }
