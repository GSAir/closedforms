// # custom traverser
  t_int __VERIFIER_nondet_int()
  t_void __VERIFIER_error()
  t_int id(t_int x){
    if (op_equals x 0) return 0
    else {}
    return (op_plus id((op_minus x 1)) 1)
  }
  t_int main(t_void ){
    t_int input = 15
    t_int result = id(input)
    if (op_notequals result 15) {
      ERROR:
      __VERIFIER_error()
    }
    else {}
  }
// # default pretty printer
  extern int __VERIFIER_nondet_int();
  extern void __VERIFIER_error();
  int id(int x)
  {
    if(x == 0)
    return 0;
    return id(x - 1) + 1;
  }
  int main(void)
  {
    int input = 15;
    int result = id(input);
    if(result != 15){
      ERROR:
      __VERIFIER_error();
    }
